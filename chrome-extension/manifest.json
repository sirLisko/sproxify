{
	"name": "Sproxify",
	"version": "0.1.3",
	"description": "Sproxify intercepts Spotify links and let you choose where to play it.",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [
				"http://open.spotify.com/*",
				"https://play.spotify.com/*",
				"https://open.spotify.com/*"
			],
			"css": ["style.css"],
			"js": ["base.js"]
		}
	],
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}